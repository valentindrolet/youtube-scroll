{
    "name": "YouTube scroll",
    "version": "1.0",
    "description": "Scroll 'Up next' section and comments section while keeping an eye to the video",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/watch*"
        ],
        "js": [
          "contentScript.js"
        ],
        "css": [
          "scrollbar.css"
        ]
      }
    ]
  }